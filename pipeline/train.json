{
  "pipeline": {
    "name": "model"
  },
  "transform": {
    "image": "combustml/pmd-training:0.1-SNAPSHOT",
    "cmd": [ 
	"/opt/docker/bin/training",
	"lending-club", 
  "-t",
  "random-forest",
	"-i", 
	"file:///pfs/training/lending_club.avro", 
	"-o", 
	"/pfs/out/model.zip", 
  "-s",
  "/pfs/out/summary.txt",
	"-J-Xmx2048m"
    ]
  },
  "parallelism_spec": {
    "strategy": "CONSTANT",
    "constant": "1"
  },
  "input": {
    "atom": {
      "repo": "training",
      "glob": "/"
    }
  }
}

