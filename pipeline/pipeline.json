{
  "pipeline": {
    "name": "model"
  },
  "transform": {
    "image": "dwhitena/pmd-training",
    "cmd": [ 
	"/opt/docker/bin/training",
	"airbnb", 
	"-t", 
	"random-forest", 
	"-i", 
	"file:///pfs/training/airbnb.clean.avro", 
	"-o", 
	"/pfs/out/model.zip", 
	"-s", 
	"/pfs/out/summary.txt", 
	"-J-Xmx2048m"
    ]
  },
  "inputs": [
    {
      "repo": {
        "name": "training"
      },
      "glob": "/"
    }
  ]
}
{
  "pipeline": {
    "name": "scoring"
  },
  "transform": {
    "image": "dwhitena/pmd-scoring",
    "cmd": [ 
	"/opt/docker/bin/scoring",
	"-m", 
	"/pfs/model/model.zip",
	"-i", 
	"/pfs/test", 
	"-o", 
	"/pfs/out",
	"-J-Xmx2048m"
    ]
  },
  "inputs": [
    {
      "repo": {
        "name": "test"
      },
      "glob": "/*"
    },
    {
      "repo": {
	"name": "model"
      },
      "glob": "/"
    }
  ]
}
