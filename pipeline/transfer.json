{
  "pipeline": {
    "name": "transfer"
  },
  "transform": {
    "image": "byrnedo/alpine-curl:latest",
    "cmd": [ 
      "curl",
      "-XPUT",
      "-H 'content-type: application/binary'",
      "--data-binary",
      "@/pfs/model/model.zip",
      "http://mleap-serving:31300"
    ]
  },
  "parallelism_spec": {
    "strategy": "CONSTANT",
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "atom": {
          "repo": "model",
          "glob": "/"
        }
      }
    ]
  }
}
